{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConfigurationValues": {
    "BackEndURL": "<BackEndUrl>",
    "SsoUrl": "<SSOUrl>",
    "TinymceKey": "<TinymceKey>"
  },
  "OAuthConfiguration": {
    "ClientId": "<Client Id>",
    "ClientSecret": "<Client Secret>",
    "Scope": "<scope>"
  },
  "OAuthConfigurationUser": {
    "ClientId": "<client Id>",
    "ClientSecret": "<Client Secret>",
    "Scope": "<scopes>",
    "CookieExpireTimeSpan": 15
  }
}
